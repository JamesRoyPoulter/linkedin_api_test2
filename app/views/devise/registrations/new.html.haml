%h2 Sign up
= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f|
  = devise_error_messages!
  %div
    = f.label :email
    %br/
    = f.email_field :email
  %div
    = f.label :number_best_to_contact_you_on
    %br/
    = f.text_field :number
  %div
    = f.label :password
    %br/
    = f.password_field :password
  %div
    = f.label :password_confirmation
    %br/
    = f.password_field :password_confirmation
  %div= f.submit "Sign up"
= render "devise/shared/links"

%div= link_to "Sign in with linkedin YOLO", user_omniauth_callback_path(:linkedin_oauth2)
%div= link_to 'YOLOLO', '/users/auth/linkedin_oauth2'